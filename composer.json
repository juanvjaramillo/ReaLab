{
    "name": "juanv/realab",
    "description": "Proyecto en php con el framework laravel y vue.js como proyecto formativo del SENA regional Caldas. Control de inventario de reactivos.",
    "type": "project",
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "juan s. villegas",
            "email": "juanvjaramillo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}